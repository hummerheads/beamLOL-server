{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)", 
            "src": "index.js" ,

            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html" 
        }
    ]
}